{
	"rules": {
		"comma-dangle": 0,
		"curly": [2, "all"],
		"indent": [2, "tab", {"SwitchCase": 1}],
		"newline-after-var": [2, "always"],
		"no-console": [2, {"allow": ["warn"]}],
		"no-extra-semi": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-trailing-spaces": [2, {"skipBlankLines": true}],
		"quote-props": 0,
		"semi": [2, "always"]
	},
	"env": {
		"browser": true,
		"es6": true,
		"jest": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": ["eslint:recommended", "prettier"]
}
